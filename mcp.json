{
  "settings": {
    "logLevel": "debug",
    "retryAttempts": 2,
    "bufferSize": "256MB",
    "maxConcurrentRequests": 3,
    "timeout": 60000
  },
  "features": {
    "performanceMetrics": false,
    "cognitiveEnhancement": true,
    "adaptiveStrategies": true,
    "parallelProcessing": false,
    "thoughtLogging": true
  },
  "servers": {
    "metacognition": {
      "enabled": true,
      "args": [
        "-y",
        "metacog"
      ],
      "command": "npx",
      "description": "MetaCognition MCP Server - Clean Install",
      "env": {
        "DISABLE_THOUGHT_LOGGING": "false",
        "COGNITIVE_ENHANCEMENT": "true",
        "CONVERGENCE_TARGET": "0.95"
      }
    }
  }
}
